      {
        "type": "paragraph",
        "content": {
          "en": ".png file, size is 32x32"
        }
      }
    ]
  },
  {
    "name": {
      "en": "App Integrations"
    },
    "settings": [
      {
        "type": "paragraph",
        "content": {
          "en": "Please read the [App Integration guide carefully to avoid unnecessary errors](https:\/\/help.arenacommerce.com\/customization\/app.html)"
        }
      },
      {
        "type": "header",
        "content": {
          "en": "WISHLIST & COMPARE APP"
        }
      },
      {
        "type": "paragraph",
        "content": {
          "en": "After enabling Wishlist & Compare App you will need to install the free install [ Wishlist & Compare app](https:\/\/wishlist.arenacommerce.com\/install)"
        }
      },
      {
        "type": "select",
        "id": "wishlist_app",
        "label": {
          "en": "Using app from"
        },
        "options": [
          {
            "value": "default",
            "label": {
              "en": "Arenacommerce app"
            }
          },
          {
            "value": "loox",
            "label": {
              "en": "Loox"
            }
          }
        ],
        "default": "default"
      },
      {
        "type": "checkbox",
        "id": "enable_product_wishlist",
        "label": {
          "en": "Enable Wishlist"
        },
        "default": true
      },
      {
        "type": "checkbox",
        "id": "enable_product_compare",
        "label": {
          "en": "Enable Compare"
        },
        "default": true
      },
      {
        "type": "checkbox",
        "id": "use_icons_from_arn_app",
        "label": {
          "en": "Use font icons from app"
        },
        "info": {
          "en": "Only for Arenacommerce app"
        }
      },
      {
        "type": "header",
        "content": {
          "en": "Product Review"
        }
      },
      {
        "type": "select",
        "id": "review_app",
        "label": {
          "en": "Product Review App"
